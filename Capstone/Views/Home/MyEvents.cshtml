@model Capstone.Models.EventsViewModels

@{
    ViewBag.Title = "My Events";
}

<h2>My Events</h2>
<hr />
<table class="table table-striped table-hover">
    <tr>
        <th>Event Title</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Location</th>
        <th>Type</th>
    </tr>
    @foreach (var item in Model.Events)
    {

        //<a href="@Url.Action("Details","Event")">
            <tr>
                <td>
                    @Html.DisplayFor(m => item.Title)
                </td>
                <td>
                    @Html.DisplayFor(m => item.StartDate)
                </td>
                <td>
                    @Html.DisplayFor(m => item.EndDate)
                </td>
                <td>
                    @Html.DisplayFor(m => item.Location)
                </td>
                <td>
                    @Html.DisplayFor(m => item.Type)
                </td>
            </tr>
       // </a>
    }

</table>